{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start -H 0.0.0.0 -p 3000",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run tests/unit",
    "test:unit:watch": "vitest watch tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:integration:watch": "vitest watch tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:e2e:watch": "vitest watch tests/e2e",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:unit": "vitest run --coverage tests/unit",
    "test:coverage:integration": "vitest run --coverage tests/integration",
    "test:coverage:e2e": "vitest run --coverage tests/e2e",
    "test:ci": "mkdir -p test-results && vitest run --coverage --reporter=verbose --reporter=junit --outputFile.junit=./test-results/junit.xml",
    "test:changed": "vitest run --changed",
    "test:related": "vitest run --related",
    "test:filter": "vitest run --grep",
    "test:bail": "vitest run --bail=1",
    "test:silent": "vitest run --silent",
    "test:verbose": "vitest run --reporter=verbose",
    "admin": "node admin-server.mjs",
    "admin-dev": "NODE_ENV=development node admin-server.mjs",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "clean": "npm cache clean --force && rm -rf node_modules && rm -rf .next && npm install",
    "setup-kokoro": "./scripts/setup-kokoro.sh",
    "dev-kokoro": "npm run setup-kokoro && next dev --port 3001"
  },
