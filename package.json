{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start -H 0.0.0.0 -p 3000",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "admin": "node admin-server.mjs",
    "admin-dev": "NODE_ENV=development node admin-server.mjs",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx scripts/seed-db.ts",
    "db:reset": "prisma migrate reset --force",
    "db:push": "prisma db push",
    "docker:dev-db": "./scripts/dev-db.sh",
    "docker:init-db": "./scripts/init-db.sh",
    "migrate-data": "tsx scripts/migrate-data.ts",
    "migrate-data:verify": "tsx scripts/migrate-data.ts --verify-only",
    "migrate-data:rollback": "tsx scripts/migrate-data.ts --rollback",
    "test-mcp": "node scripts/test-mcp.js",
    "fix-mcp": "npm cache clean --force && node scripts/test-mcp.js",
    "fix-mcp-sudo": "sudo npm cache clean --force && sudo npm install -g minimax-mcp-js && node scripts/test-mcp.js",
    "mcp-health": "node scripts/mcp-health-check.js",
    "clean": "npm cache clean --force && rm -rf node_modules && rm -rf .next && npm install",
    "setup-kokoro": "./scripts/setup-kokoro.sh",
    "dev-kokoro": "npm run setup-kokoro && next dev --port 3001"
  },
